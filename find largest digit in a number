#include <stdio.h>

int main() {
    int num = 123;
    int i= 0;
    int digit ;
    int smallest= 9;
    
    while (num > i){
        digit = num % 10;
        if( digit < smallest ){
            smallest = digit;
        }
        num = num / 10;
    }
    
    printf("smallest number : %d",smallest);

    return 0;
}
