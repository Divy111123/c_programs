#include <stdio.h>

int main() {
    int num = 234;
    int smallest = 9;
    int digit;

    while (num > 0) {
        digit = num % 10;        // last digit
        if (digit < smallest) {
            smallest = digit;     // update largest
        }
        num = num / 10;          // remove last digit
    }

    printf("smallest  digit is: %d\n", smallest);
    return 0;
}
