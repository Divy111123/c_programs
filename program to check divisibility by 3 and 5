#include <stdio.h>
int main(){
      int num;
    printf(" enter the number : ");
    scanf("%d",&num);
    if(num % 5==0 && num %3==0){
        printf("The number divisible by 5 and 3");
    }
    else if(num % 5==0 ){
        printf("The number is divisible by 5");
    }
    else if(num % 3==0 ){
        printf("The number is divisible by 3");
    }
    else{
        printf("The number is never divisible by 5 and 3");
    }
    return 0;
}
